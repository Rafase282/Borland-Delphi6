    Button1: TButton;
    procedure FormActivate(Sender: TObject);
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form3: TForm3;

implementation

uses gauge282, chat;

{$R *.dfm}

procedure TForm3.FormActivate(Sender: TObject);
begin
Form1.hide;
end;

